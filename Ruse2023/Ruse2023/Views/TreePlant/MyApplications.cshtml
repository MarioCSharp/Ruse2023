@using Ruse2023.Models.TreePlant;
@model List<TreePlantApprovalModel>

<div class="list-group">
    @foreach (var app in Model)
    {
        <p class="list-group-item list-group-item-action">
            @app.Description | <a asp-action="ApplicationDetails" asp-controller="TreePlant" asp-route-id="@app.Id">Details</a>
        </p>
    }
</div>